{
  "name": "tcr-ui",
  "author": "kangarang",
  "version": "0.0.1",
  "description": "Client-side shell to interact with token-curated registries",
  "repository": "https://github.com/kangarang/tcr-ui",
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "dev": "yarn run lint && react-scripts start",
    "fix": "eslint --fix ./src",
    "lint": "eslint ./src",
    "precommit": "lint-staged",
    "rpc": "ganache-cli --port 8545 -i 420 --mnemonic 'candy maple cake sugar pudding cream honey rich smooth crumble sweet treat'",
    "start": "yarn run dev",
    "test": "react-scripts test --env=jsdom",
    "update:tokens": "babel-node scripts/update-tokens",
    "update:abis": "babel-node scripts/addABIs",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@material-ui/core": "1.4.3",
    "@material-ui/icons": "2.0.1",
    "babel-polyfill": "6.26.0",
    "bn.js": "4.11.8",
    "date-fns": "1.29.0",
    "dayjs": "1.6.4",
    "ethereum-ens": "0.7.4",
    "ethereumjs-util": "5.2.0",
    "ethers": "3.0.26",
    "ethjs": "0.4.0",
    "ethjs-abi": "0.2.1",
    "ethjs-contract": "0.2.3",
    "file-saver": "1.3.8",
    "fs-extra": "5.0.0",
    "immutable": "3.8.2",
    "ipfs-mini": "1.1.2",
    "jazzicon": "1.5.0",
    "localforage": "1.7.2",
    "lodash": "4.17.10",
    "moment": "2.22.0",
    "node-uuid": "1.4.8",
    "path": "0.12.7",
    "react": "16.4.2",
    "react-copy-to-clipboard": "5.0.1",
    "react-dom": "16.4.2",
    "react-loadable": "5.4.0",
    "react-markdown": "3.3.0",
    "react-motion": "0.5.2",
    "react-notification-system": "0.2.17",
    "react-notification-system-redux": "1.2.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "4.0.8",
    "react-scripts": "1.1.4",
    "redux": "4.0.0",
    "redux-immutable": "4.0.0",
    "redux-saga": "0.16.0",
    "redux-saga-test-plan": "3.6.0",
    "reselect": "3.0.1",
    "styled-components": "3.4.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.8",
    "@storybook/addon-links": "3.4.8",
    "@storybook/addons": "3.4.8",
    "@storybook/react": "3.4.8",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.3",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "eslint": "4.19.1",
    "eslint-plugin-lodash-fp": "2.1.3",
    "eslint-plugin-react": "7.7.0",
    "ganache-cli": "6.1.0",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "lint-staged": "7.0.0",
    "prettier": "1.14.1",
    "react-test-renderer": "16.3.1",
    "redux-logger": "3.0.6"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
