{
  "name": "tcr-ui",
  "author": "kangarang",
  "version": "0.0.7",
  "description": "Client-side shell to interact with token-curated registries",
  "repository": "https://github.com/kangarang/tcr-ui",
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "start:prod": "npm run build && node server.js",
    "dev": "npm run lint && react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "build": "react-scripts build",
    "precommit": "lint-staged",
    "fix": "eslint --fix ./src",
    "lint": "eslint ./src",
    "update:tokens": "babel-node scripts/update-tokens"
  },
  "dependencies": {
    "@material-ui/icons": "1.0.0",
    "axios": "^0.18.0",
    "babel-polyfill": "6.26.0",
    "bn.js": "4.11.8",
    "date-fns": "1.29.0",
    "ethers": "^3.0.15",
    "ethjs": "^0.4.0",
    "ethjs-abi": "^0.2.1",
    "ethjs-contract": "0.2.3",
    "express": "4.16.3",
    "file-saver": "1.3.8",
    "fs-extra": "6.0.1",
    "immutable": "3.8.2",
    "ipfs-mini": "1.1.2",
    "jazzicon": "1.5.0",
    "lodash": "4.17.10",
    "material-ui": "1.0.0-beta.42",
    "moment": "2.22.1",
    "path": "0.12.7",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-loadable": "5.4.0",
    "react-markdown": "3.3.2",
    "react-motion": "0.5.2",
    "react-notification-system": "0.2.17",
    "react-notification-system-redux": "1.2.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "react-scripts": "1.1.4",
    "redux": "4.0.0",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "redux-saga-test-plan": "3.6.0",
    "reselect": "3.0.1",
    "styled-components": "3.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.4",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "^6.26.0",
    "eslint": "4.19.1",
    "eslint-plugin-lodash-fp": "2.1.3",
    "eslint-plugin-react": "7.8.2",
    "husky": "0.14.3",
    "jest": "22.4.4",
    "lint-staged": "7.1.1",
    "prettier": "1.12.1",
    "react-test-renderer": "16.3.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
